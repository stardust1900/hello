package Tools;

public class DebugPrint {
	
	public static void  DebugPrint(String message)
	{
		if(true)
		{
			System.out.println("Debug: "+message);
		}
	}
	
	public static void  InforPrint(String message)
	{
		if(true)
		{
			System.out.println("Infor:  "+message);
		}
	}

}
